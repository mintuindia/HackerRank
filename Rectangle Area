
/*
 * Create classes Rectangle and RectangleArea
 */
    class Rectangle{
      public:
        int width,height;
        int area;
        void read_input(){
            cin>>width;
            cin>>height;
        }
        void display(){
            cout<<width<<" ";
            cout<<height<<"\n";
        }
    };
    class RectangleArea:public Rectangle{
      public:
        void display(){
            area=(width*height);
            cout<<area;
        }
    };
